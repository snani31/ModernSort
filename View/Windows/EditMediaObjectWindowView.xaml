<customCtrs:BaseWindow x:Class="ModernSort.View.Windows.EditMediaObjectWindowView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:customUI="clr-namespace:ModernSort.CustomeControls"
        xmlns:local="clr-namespace:ModernSort.View.Windows"
        xmlns:customCtrs ="clr-namespace:ModernSort.CustomeControls.CustomUIControls"
        xmlns:enums="clr-namespace:ModernSort.Enums"
        mc:Ignorable="d"
        Title="EditMediaObjectWindowView" 
        Height="520" 
        Width="900"
        WindowStyle="none"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        AllowsTransparency="True"
        Background="Transparent"
        ClipToBounds="False"
         MouseLeftButtonDown="Window_DragHandler"
        >
    <Border
       
Background= "{DynamicResource MeinBackgroundColor}"
CornerRadius="{Binding RelativeSource={RelativeSource AncestorType=customCtrs:BaseWindow,Mode=FindAncestor},Path=MainBorderCornerRadius}"
ClipToBounds="True"
    >
        <Grid>
            <Grid ClipToBounds="True">
                <Grid.RowDefinitions>
                    <RowDefinition 
                    Height="50"
             />
                    <RowDefinition/>
                </Grid.RowDefinitions>

                <Border
                Grid.Column="0"
                Grid.ColumnSpan="1"
                d:CornerRadius="20,20,0,0"
                CornerRadius="{Binding RelativeSource={RelativeSource AncestorType=customCtrs:BaseWindow,Mode=FindAncestor},Path=TopButtonsBarBorderCornerRadius}"
                Background="{DynamicResource MeinThemeColor}"
                BorderThickness="0"
         >
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="50"/>
                            <ColumnDefinition Width="50"/>
                            <ColumnDefinition Width="50"/>
                        </Grid.ColumnDefinitions>


                        <Button Grid.Column="1"
              ToolTip="Hide"
                 Click="HideButton_Click">
                            <Button.Template>
                                <ControlTemplate TargetType="Button">
                                    <Border BorderThickness="0" Margin="0" Name="brd" Background="Transparent" CornerRadius="10 20 0 10">
                                        <Image Margin="8" Source="{DynamicResource MinimizeBordered}"/>
                                    </Border>
                                    <ControlTemplate.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter TargetName="brd" Property="Background" Value="{DynamicResource DimMeinThemeColor}"/>
                                        </Trigger>
                                    </ControlTemplate.Triggers>
                                </ControlTemplate>
                            </Button.Template>
                        </Button>

                        <Button Grid.Column="2"
              ToolTip="Maximize"
                 Click="MaximizeButton_Click">
                            <Button.Template>
                                <ControlTemplate TargetType="Button">
                                    <Border BorderThickness="0" Margin="0" Name="brd" Background="Transparent" CornerRadius="10 20 0 10">
                                        <Image Margin="8" Source="{DynamicResource MaximizeBordered}"/>
                                    </Border>
                                    <ControlTemplate.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter TargetName="brd" Property="Background"  Value="{DynamicResource DimMeinThemeColor}"/>
                                        </Trigger>
                                    </ControlTemplate.Triggers>
                                </ControlTemplate>
                            </Button.Template>
                        </Button>

                        <Button Grid.Column="3"
             Command="{Binding CloseDialog}"
             ToolTip="Close">
                            <Button.Template>
                                <ControlTemplate TargetType="Button">
                                    <Border BorderThickness="0" Margin="0" Name="brd" Background="Transparent" CornerRadius="{Binding RelativeSource={RelativeSource AncestorType=customCtrs:BaseWindow,Mode=FindAncestor},Path=TopRightCloseButtonBorderCornerRadius}">
                                        <Image Margin="10" Source="{DynamicResource PowerOffMeinBackgroundImage}"/>
                                    </Border>
                                    <ControlTemplate.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter TargetName="brd" Property="Background" Value="{DynamicResource ContrastingColor}"/>
                                        </Trigger>
                                    </ControlTemplate.Triggers>
                                </ControlTemplate>
                            </Button.Template>
                        </Button>

                    </Grid>
                </Border>

                <Grid
                Grid.Row="1"
                >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>

                    <Grid
                        x:Name="FilterFilePresenterGrid"
                    Grid.Column="1"
                    >
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="120"/>
                        </Grid.RowDefinitions>


                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition 
                                    Width="75 "/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>

                            <Border
                            CornerRadius="20 0 0 20"
                            Grid.Column="0"
                            Grid.RowSpan="2"
                            >
                                <Grid
                                    xmlns:l="clr-namespace:ModernSort.CustomeControls.TemplateSelectors"
                                    Background="{DynamicResource MeinColorBackgroundFillImage}"
                                    >
                                    <Grid.Resources>
                                        <DataTemplate
                                            x:Name="Files"
                                            x:Key="Files">
                                            <Border
                                                DataContext="{Binding DataContext,
                                                RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Window}}"
                                                Background="Transparent"
                                                >
                                                <Grid>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="100"/>
                                                        <RowDefinition/>
                                                    </Grid.RowDefinitions>

                                                    <customCtrs:CustomButton
                                                        FirstButtonColor="{DynamicResource MeinThemeColor}"
                                                        SecondButtonColor="{DynamicResource MeinBackgroundColor}"
                                                        Grid.Row="0"
                                                        BorderThickness="0"
                                                        FontSize="30"
                                                        Text="Select File"
                                                        ImageMargin="15"
                                                        Command="{Binding SelectMultimediaFiles}"
                                                        TextBlockHorizontalAlignment ="Center"
                                                        DisabledImageSource="{DynamicResource SelectFolderBordered2}"
                                                        DefoultImageSource="{DynamicResource SelectFolderMein}"
                                                        MouseHoverImageSource="{DynamicResource SelectFolderBordered}"
                                                        />

                                                    <ListView
                                                        Grid.Row="1"
                                                        ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                                                        ScrollViewer.VerticalScrollBarVisibility="Auto"
                                                        HorizontalContentAlignment="Center"
                                                        x:Name="MediaFilesList"
                                                        ItemContainerStyle="{StaticResource MediaObjactContenerStyle}"
                                                        ItemsSource="{Binding SelectedFilePaths}"
                                                        BorderThickness="0"
                                                        Background="Transparent"
                                                        Padding="0 10 0 0"
                                                        >
                                                        <ListView.Style>
                                                            <Style
                                                                TargetType="ListView"
                                                                BasedOn="{StaticResource RoundedListViewStyle}">
                                                                <Style.Setters>
                                                                    <Setter Property="Template">
                                                                        <Setter.Value>
                                                                            <ControlTemplate TargetType="ListView">
                                                                                <Border
                                                                                    Name="brd"
                                                                                    CornerRadius="0 0 0 20"
                                                                                    Background="{TemplateBinding Background}"
                                                                                    BorderThickness="{TemplateBinding BorderThickness}"
                                                                                    >
                                                                                    <ScrollViewer Padding="{TemplateBinding Padding}">
                                                                                        <ItemsPresenter/>
                                                                                    </ScrollViewer>
                                                                                </Border>
                                                                                <ControlTemplate.Triggers>
                                                                                    <Trigger Property="Validation.HasError" Value="True">
                                                                                        <Setter TargetName="brd" Property="BorderThickness" Value="2 0 0 2"/>
                                                                                        <Setter TargetName="brd" Property="BorderBrush" Value="{DynamicResource ContrastingColor}"/>
                                                                                    </Trigger>
                                                                                </ControlTemplate.Triggers>
                                                                            </ControlTemplate>
                                                                        </Setter.Value>
                                                                    </Setter>
                                                                </Style.Setters>
                                                            </Style>
                                                        </ListView.Style>
                                                        <ListView.ItemTemplate>

                                                            <DataTemplate>
                                                                <Border
                                                                Background="{DynamicResource MeinThemeColor}"
                                                                x:Name="brd" 
                                                                Width="150" 
                                                                MinHeight="200" 
                                                                CornerRadius="15 15 10 10">
                                                                    <Grid>
                                                                        <Grid.RowDefinitions>
                                                                            <RowDefinition Height="50"/>
                                                                            <RowDefinition Height="*"/>
                                                                        </Grid.RowDefinitions>
                                                                        <Border 
                                                                        x:Name="InBrd"
                                                                        Grid.Row="0"
                                                                        CornerRadius="10 10 0 0"
                                                                        Background="{DynamicResource DimBackgroundColor}">
                                                                            <StackPanel
                                                                            Orientation="Horizontal"
                                                                            HorizontalAlignment="Right"
                                                                            >

                                                                                <customCtrs:CustomButton
                                                                                    Grid.Column="0"
                                                                                    FirstButtonColor="{DynamicResource MeinThemeColor}"
                                                                                    SecondButtonColor="{DynamicResource DimBackgroundColor}"
                                                                                    CornerRadius="0 10 0 0"
                                                                                    ToolTip="Remove file"
                                                                                    ImageMargin="5"
                                                                                    
                                                                                    Command="{Binding DataContext.RemoveFileFromList, 
                                                                                    RelativeSource={RelativeSource AncestorType=ListView, AncestorLevel=1}}"
                                                                                    CommandParameter="{Binding}"
                                                                                    
                                                                                    TextBlockHorizontalAlignment ="Left"
                                                                                    DefoultImageSource="{DynamicResource RoundedCrossContrastinng}"
                                                                                    MouseHoverImageSource="{DynamicResource RoundedCrossFillContrastinng}"
                                                                                    
                                                                                    />
                                                                            </StackPanel>
                                                                        </Border>
                                                                        <Border
                                                                            CornerRadius="0 0 10 10"
                                                                            x:Name="ImgBrd"
                                                                            Grid.Row="1">
                                                                            <Border.Background>
                                                                                <ImageBrush 
                                                                                Stretch="Fill"
                                                                                ImageSource="{Binding ., Converter={StaticResource ToBitmapConverter}}"/>
                                                                            </Border.Background>
                                                                        </Border>
                                                                    </Grid>
                                                                </Border>
                                                                <DataTemplate.Triggers>
                                                                    <MultiTrigger>
                                                                        <MultiTrigger.Conditions>
                                                                            <Condition Property="IsMouseOver" Value="True" />
                                                                        </MultiTrigger.Conditions>
                                                                        <Setter TargetName="ImgBrd" Property="Margin" Value="10" />
                                                                        <Setter TargetName="ImgBrd" Property="CornerRadius" Value="10" />
                                                                    </MultiTrigger>
                                                                </DataTemplate.Triggers>
                                                            </DataTemplate>
                                                        </ListView.ItemTemplate>
                                                        <ListView.Resources>
                                                            <Style TargetType="ScrollBar" BasedOn="{StaticResource WhiteScrollBarStyle}"/>
                                                        </ListView.Resources>
                                                        <ListView.ItemsPanel>
                                                            <ItemsPanelTemplate>
                                                                <WrapPanel 
                                                                Orientation="Horizontal"  
                                                                HorizontalAlignment="Center"
                                                                />
                                                            </ItemsPanelTemplate>
                                                        </ListView.ItemsPanel>
                                                    </ListView>
                                                </Grid>
                                            </Border>
                                        </DataTemplate>
                                        
                                        <DataTemplate
                                            x:Key="Filters">
                                            <Border
                                            DataContext="{Binding DataContext,
                                            RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Window}}"
                                            Background="Transparent"
                                            >
                                                <ScrollViewer
                                                    PreviewMouseWheel ="ScrollViewer_PreviewMouseWheel"
                                                    CanContentScroll="False"
                                                    SnapsToDevicePixels="True"
                                                    HorizontalScrollBarVisibility="Disabled"
                                                    VerticalScrollBarVisibility="Auto"
                                                    >
                                                    <ScrollViewer.Resources>
                                                        <Style TargetType="ScrollBar" BasedOn="{StaticResource WhiteScrollBarStyle}"/>
                                                    </ScrollViewer.Resources>
                                                    <ListView
                                                    xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
                                                    x:Name="FiltersCriterionsList"
                                                    ScrollViewer.CanContentScroll="False"
                                                    ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                                                    ScrollViewer.VerticalScrollBarVisibility="Auto"
                                                    HorizontalContentAlignment="Stretch"
                                                    ItemContainerStyle="{StaticResource MediaObjactContenerStyle}"
                                                    ItemsSource="{Binding FilterCriterions}"
                                                    BorderThickness="0"
                                                    Background="Transparent"
                                                    Padding="0 10 0 0"
                                                    >
                                                        <ListView.ItemTemplate>

                                                            <DataTemplate>

                                                                <Border
                                                                MinHeight="350"
                                                                Background="{DynamicResource DimBackgroundColor}"
                                                                CornerRadius="40"
                                                                Margin="20"
                                                                >
                                                                    <StackPanel
                                                                    Orientation="Vertical"
                                                                    >
                                                                        <Border 
                                                                        MinHeight="75"
                                                                        Background="{DynamicResource MeinThemeColor}"
                                                                        CornerRadius="10">
                                                                            <TextBlock
                                                                            x:Name="FilterCriterionTytleTextBlock"
                                                                            TextWrapping="Wrap"
                                                                            Foreground="{DynamicResource MeinBackgroundColor}"
                                                                            FontSize="25"
                                                                            Style="{StaticResource SpanDefoult}"
                                                                            TextAlignment="Center"
                                                                            VerticalAlignment="Center"
                                                                            HorizontalAlignment="Center"
                                                                            Text="{Binding Tytle}"
                                                                            />
                                                                        </Border>

                                                                        <ListView
                                                                        ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                                                                        ScrollViewer.VerticalScrollBarVisibility="Disabled"
                                                                        MinHeight="200"
                                                                        HorizontalContentAlignment="Stretch"
                                                                        BorderThickness="0"
                                                                        ItemsSource="{Binding FilterItems}"
                                                                        Background="Transparent"
                                                                        Padding="0 10 0 30"
                                                                        ItemContainerStyle="{StaticResource RankingListItemStyle}"
                                                                          >
                                                                            <ListView.ItemTemplate>
                                                                                <DataTemplate>
                                                                                    <Grid
                                                                                    HorizontalAlignment="Stretch"
                                                                                    >
                                                                                        <Grid.RowDefinitions>
                                                                                            <RowDefinition Height="50"/>
                                                                                        </Grid.RowDefinitions>
                                                                                        <Grid.ColumnDefinitions>
                                                                                            <ColumnDefinition Width="*"/>
                                                                                            <ColumnDefinition Width="50"/>
                                                                                        </Grid.ColumnDefinitions>

                                                                                        <TextBlock
                                                                                        HorizontalAlignment="Left"
                                                                                        
                                                                                        Foreground="{Binding 
                                                                                        Path=Foreground, 
                                                                                        RelativeSource={RelativeSource AncestorType=ListViewItem,Mode=FindAncestor}}"
                            
                                                                                        Grid.Row="0"
                                                                                        Grid.Column="0"
                                                                                        x:Name="TytleTbx"
                                                                                        FontSize="20"
                                                                                        TextWrapping="Wrap"
                                                                                        Background="Transparent"
                                                                                        Text="{Binding ConditionTytle}"
                                                                                        Width="150"
                                                                                        Height="50"
                                                                                        Margin="30 0 0 0"
                                                                                        />
                                                                                        <Viewbox
                                                                                        Grid.Row="0"
                                                                                        Grid.Column="1"
                                                                                        Width="50"
                                                                                        Height="50"
                                                                                        HorizontalAlignment="Right"
                                                                                        >
                                                                                            <CheckBox
                                                                                            IsChecked="{Binding IsFilterSelected, Mode=TwoWay}"
                                                                                            BorderBrush="{DynamicResource MeinThemeColor}"
                                                                                            Background="{DynamicResource MeinBackgroundColor}"
                                                                                            Style="{StaticResource CheckBoxRoundenStyle}"
                                                                                             >
                                                                                                <i:Interaction.Triggers>
                                                                                                    <i:EventTrigger EventName="Checked">
                                                                                                        <i:InvokeCommandAction
                                                                                                        Command="{Binding DataContext.AddFilterToSelection, RelativeSource={RelativeSource AncestorLevel=2,AncestorType=ListView,Mode=FindAncestor}}"
                                                                                                        CommandParameter="{Binding}"/>
                                                                                                    </i:EventTrigger>
                                                                                                    <i:EventTrigger EventName="Unchecked">
                                                                                                        <i:InvokeCommandAction
                                                                                                        Command="{Binding DataContext.RemoveFilterFromSelection, RelativeSource={RelativeSource AncestorLevel=2,AncestorType=ListView,Mode=FindAncestor}}"
                                                                                                        CommandParameter="{Binding}"/>
                                                                                                    </i:EventTrigger>
                                                                                                </i:Interaction.Triggers>
                                                                                            </CheckBox>

                                                                                        </Viewbox>

                                                                                    </Grid>
                                                                                </DataTemplate>
                                                                            </ListView.ItemTemplate>
                                                                        </ListView>

                                                                    </StackPanel>
                                                                </Border>

                                                            </DataTemplate>
                                                        </ListView.ItemTemplate>
                                                        <ListView.ItemsPanel>
                                                            <ItemsPanelTemplate>
                                                                <StackPanel 
                                                                Orientation="Vertical"
                                                                VerticalAlignment="Top"
                                                                HorizontalAlignment="Stretch"
                                                                />
                                                            </ItemsPanelTemplate>
                                                        </ListView.ItemsPanel>
                                                    </ListView>
                                                </ScrollViewer>
                                            </Border>
                                        </DataTemplate>
                                        
                                        <l:EditMediaObjectFunctionTemplateSelector
                                            x:Key="FunctionSelector"
                                            FilesTemplate="{StaticResource Files}"
                                            FiltersTemplate="{StaticResource Filters}"
                                            />
                                    </Grid.Resources>
                                    <ContentControl
                                        x:Name="SelectFunktionPrtesenter"
                                        ContentTemplateSelector="{StaticResource FunctionSelector}"
                                        Content="{Binding Path=PagePresenter}"
                                        >
                                    </ContentControl>
                                </Grid>
                            </Border>

                            <Border
                                Background="{DynamicResource DimBackgroundColor}"
                                Grid.Column="1"
                                Grid.RowSpan="2"
                                >
                                
                            </Border>

                            <customCtrs:CustomRadioButton
                                Command="{Binding ShowMediaFilesPage}" 
                                Grid.Column="1"
                                Grid.Row="0"
                                
                                DirrectionType="VerticalLeftDirrection"
                                
                                DefoultImageSource="{DynamicResource GalleryBordered}"
                                DisabledImageSource="{DynamicResource GalleryBordered}"
                                SelectedImageSource="{DynamicResource GalleryMein}"
                                MouseHoverImageSource="{DynamicResource GalleryBordered}"
                                
                                DefoultColor="{DynamicResource MeinThemeColor}"
                                MouseEnterColor="{DynamicResource ContrastingColor}"
                                SelectedColor="{DynamicResource MeinBackgroundColor}"
                                DisabledColor ="{DynamicResource DimMeinThemeColor3}"
                                
                                DefoultTextColor="{DynamicResource MeinBackgroundColor}"
                                SelectedTextColor="{DynamicResource MeinThemeColor}"
                                
                                IndicatorLineColor="{DynamicResource DimBackgroundColor}"
                                />

                            <customCtrs:CustomRadioButton
                                Command="{Binding ShowFiltersPage}" 
                                Grid.Column="1"
                                Grid.Row="1"
                                
                                DirrectionType="VerticalLeftDirrection"
                                
                                DefoultImageSource="{DynamicResource FilterBordered}"
                                DisabledImageSource="{DynamicResource FilterBordered}"
                                SelectedImageSource="{DynamicResource FilterMein}"
                                MouseHoverImageSource="{DynamicResource FilterBordered}"
                                
                                DefoultColor="{DynamicResource MeinThemeColor}"
                                MouseEnterColor="{DynamicResource ContrastingColor}"
                                SelectedColor="{DynamicResource MeinBackgroundColor}"
                                DisabledColor ="{DynamicResource DimMeinThemeColor3}"
                                
                                DefoultTextColor="{DynamicResource MeinBackgroundColor}"
                                SelectedTextColor="{DynamicResource MeinThemeColor}"
                                
                                IndicatorLineColor="{DynamicResource DimBackgroundColor}"
                                />

                        </Grid>

                        <Border
                        Margin="0 10 0 0"
                        d:CornerRadius="60  0 20 0"
                            CornerRadius="{Binding RelativeSource={RelativeSource AncestorType=customCtrs:BaseWindow,Mode=FindAncestor},Path=BottonRightElementBorderCornerRadius}"
                        Background="{DynamicResource MeinThemeColor}"
                        Grid.Row="1"
                        >
                            <TextBlock
                            Margin="15 0 20 0"
                            VerticalAlignment="Center"
                            TextWrapping="Wrap"
                            Foreground="{DynamicResource MeinBackgroundColor}"
                            FontSize="20"
                            Style="{StaticResource SpanDefoult}"
                            >
                                <TextBlock.Text>
                                    <MultiBinding Converter="{StaticResource ValidationStringConverter}">
                                        <Binding
                                            ElementName="FilterFilePresenterGrid" 
                                            Path= "(Validation.Errors)[0].ErrorContent"
                                            Mode="OneWay"
                                            />
                                        <Binding/>
                                    </MultiBinding>
                                </TextBlock.Text>
                            </TextBlock> 
                        </Border>

                    </Grid>

                    <Grid
                    Grid.Column="0"
                    >
                        <Grid.RowDefinitions>
                            <RowDefinition Height="100"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="120"/>
                        </Grid.RowDefinitions>

                        <Grid
                        Grid.Row="0"
                        >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            
                            <customCtrs:CustomButton
                                FirstButtonColor="{DynamicResource ContrastingColor}"
                                SecondButtonColor="{DynamicResource MeinBackgroundColor}"
                                CornerRadius="10"
                                BorderThickness="5"
                                ToolTip="Delete media object"
                                ImageMargin="10"
                                Command="{Binding DeleteMediaObjact}"
                                TextBlockHorizontalAlignment ="Left"
                                DefoultImageSource="{DynamicResource RemoveMultimediaContrasting}"
                                MouseHoverImageSource="{DynamicResource RemoveMultimediaBordered}"
                                Margin="10"
                                Grid.Row="0"
                                />
                                
                            <TextBox 
                            Grid.Column="1"
                            x:Name="TytleTbx"
                            ToolTip="Category Tytle"
                            FontSize="30"
                            Foreground="{DynamicResource MeinThemeColor2}"
                            TextWrapping="Wrap"
                            Background="{DynamicResource DimBackgroundColor}"
                            Text="{Binding MediaObjectTytle, Mode=TwoWay}"
                            Style="{StaticResource TuskDataTextBoxStyle}"
                            Margin="10"
                            Padding="10"
                            />
                        </Grid>

                        <TextBox
                            x:Name="DescryptioonTbx"
                            ToolTip="Category  Descryption"
                            Grid.Row="1"
                            FontSize="30"
                            Foreground="{DynamicResource MeinThemeColor2}"
                            TextWrapping="Wrap"
                            Background="{DynamicResource DimBackgroundColor}"
                            Text="{Binding MediaObjectDescryption, Mode=TwoWay}"
                            Style="{StaticResource TuskDataTextBoxStyle}"
                            Margin="10"
                            Padding="10"
                            />


                        <customCtrs:CustomButton
                            FirstButtonColor="{DynamicResource MeinThemeColor}"
                            SecondButtonColor="{DynamicResource MeinBackgroundColor}"
                            DisabledStateColor="{DynamicResource DimBackgroundColor}"
                            Grid.Row="2"
                            BorderThickness="4"
                            FontSize="30"
                            CornerRadius="10"
                            Text="Apply Edit"
                            Margin="10"
                            ImageMargin="5"
                            Command="{Binding EditMediaObject}"
                            TextBlockHorizontalAlignment ="Center"
                            DisabledImageSource="{DynamicResource EditGalleryBordered2}"
                            DefoultImageSource="{DynamicResource EditGalleryMein}"
                            MouseHoverImageSource="{DynamicResource EditGalleryBordered}"
                            />

                    </Grid>

                </Grid>

            </Grid>

        </Grid>
    </Border>
</customCtrs:BaseWindow>
