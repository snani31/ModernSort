<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="AppResources\CollapseWindpwIcon.png" />
    <None Remove="AppResources\PowerOffButtonIcon.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="AppResources\CollapseWindpwIcon.png" />
    <Resource Include="AppResources\PowerOffButtonIcon.png" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="StaticData\" />
    <Folder Include="Stores\" />
    <Folder Include="View\" />
    <Folder Include="CustomeControls\" />
  </ItemGroup>

  <ItemGroup>
    <Page Update="MainWindow.xaml">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Page>
  </ItemGroup>

</Project>
